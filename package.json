{
  "name": "github-clone-backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "tsc-watch --onSuccess \"node --env-file=.env  ./dist/server.js\"",
    "start": "node --env-file=.env ./dist/server.js",
    "build": "tsc",
    "db:studio": "drizzle-kit studio --config=src/app/configs/drizzle.config.ts",
    "db:seed": "tsc && node --env-file=.env ./dist/app/database/seeders/database.seeder.js",
    "db:generate": "npx drizzle-kit generate --config=src/app/configs/drizzle.config.ts",
    "db:migrate": "npx drizzle-kit migrate --config=src/app/configs/drizzle.config.ts"
  },

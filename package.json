    "@upstash/semantic-cache": "^1.0.4",
    "@upstash/vector": "^1.1.6",
    "axios": "^1.7.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.31.0",
    "elevenlabs": "^0.16.0",
